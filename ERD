Departments
-
Department_No INTEGER PK
Department_Name VARCHAR 
 

Department_Manager
-
Department_No INTEGER FK >- Departments.Department_No
Employee_No INTEGER FK - Employee.Employee_No 
From_Date INTEGER 
To_Date INTEGER

Department_Employee
-
Employee_No INTEGER FK - Employee.Employee_No
Department_No INTEGER FK >- Departments.Department_No
From_Date INTEGER 
To_Date INTEGER

Employee
-
Employee_No INTEGER PK
Birthday INTEGER
First_Name VARCHAR
Last_Name VARCHAR
Gender ENUM(M,F)
Hire_Date INTEGER

Titles
-
Employee_No INTEGER FK - Employee.Employee_No
Title VARCHAR 
From_Date INTEGER
To_Date INTEGER


Salaries
-
Employee_No INTEGER FK - Employee.Employee_No
Salary INTEGER
From_Date INTEGER
To_Date INTEGER
